<p>CC = gcc<br>CCFLAGS = -O3<br>LDFLAGS = -lm</p>
<p>all: mmc&nbsp;</p>
<p>mmc: main.o mm.o<br>&nbsp; &nbsp; $(CC) -o $@ $^ $(LDFLAGS)<br>##$(CC) -o mmc main.o mm.o $(LDFLAGS) &nbsp; &nbsp;&nbsp;</p>
