<p>main.o: main.c<br>&nbsp; &nbsp; $(CC) -c $(CCFLAGS) $&lt;<br>##$(CC) -c $(CCFLAGS) main.c</p>
<p>clean:<br>&nbsp; &nbsp; rm *.o &nbsp;</p>
